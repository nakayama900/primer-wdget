{
  "name": "primer-solid",
  "version": "0.0.0",
  "description": "An implementation of GitHub's Primer Design System using React",
  "main": "lib/index.js",
  "module": "lib-esm/index.js",
  "exports": {
    ".": {
      "import": "./lib-esm/index.js",
      "require": "./lib/index.js"
    },
    "./drafts": {
      "import": "./lib-esm/drafts/index.js",
      "require": "./lib/drafts/index.js"
    },
    "./deprecated": {
      "import": "./lib-esm/deprecated/index.js",
      "require": "./lib/deprecated/index.js"
    },
    "./lib-esm/*": {
      "import": [
        "./lib-esm/*.js",
        "./lib-esm/*/index.js"
      ],
      "require": [
        "./lib/*.js",
        "./lib/*/index.js"
      ]
    }
  },
  "typings": "lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview"
  },
  "license": "MIT",
  "devDependencies": {
    "@primer/octicons": "^17.9.0",
    "@types/chroma-js": "^2.1.4",
    "@types/node": "^18.11.9",
    "chroma-js": "^2.4.2",
    "solid-styled-components": "^0.28.5",
    "typescript": "^4.9.3",
    "vite": "^3.2.10",
    "vite-plugin-solid": "^2.4.0"
  },
  "dependencies": {
    "@github/combobox-nav": "^2.1.5",
    "@github/markdown-toolbar-element": "^2.1.1",
    "@github/paste-markdown": "^1.4.2",
    "@primer/behaviors": "^1.3.1",
    "@primer/css": "^20.6.0",
    "@primer/primitives": "7.10.0",
    "@solidjs/router": "^0.5.0",
    "@styled-system/css": "^5.1.5",
    "@styled-system/props": "^5.1.5",
    "@styled-system/theme-get": "^5.1.2",
    "@types/styled-components": "^5.1.26",
    "@types/styled-system": "^5.1.15",
    "@types/styled-system__css": "^5.0.17",
    "@types/styled-system__theme-get": "^5.0.2",
    "classnames": "^2.3.2",
    "color2k": "^1.2.5",
    "deepmerge": "^4.2.2",
    "focus-visible": "^5.2.0",
    "fzy.js": "0.4.1",
    "history": "^5.3.0",
    "lodash.isempty": "4.4.0",
    "lodash.isobject": "3.0.2",
    "solid-js": "^1.6.2",
    "styled-system": "^5.1.5"
  }
}
